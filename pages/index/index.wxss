.title {
  display: block;
  text-align: left;
  padding-bottom: 10rpx;
  margin-bottom: 20rpx;
  border-bottom: 1rpx solid rgba(70, 70, 70, 0.1);
  font-size: 30rpx;
  color: #333;
}

.baseInfo {
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  width: 100%;
  height: 200rpx;
}

.baseInfoHeader {
  width: 100rpx;
  height: 100rpx;
  border-radius: 100%;
}

.baseInfoDetail {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 630rpx;
  height: 100%;
  margin-left: 20rpx;
  font-size: 24rpx;
}

.baseInfoDetail view {
  height: 25%;
  

}
.baseInfoDetail  text{
  padding-bottom: 10rpx;
text-decoration:underline;
}

.boyhood {
  font-size: 20rpx;
}

.adolescence {
  font: 20rpx;
}

.maturity {
  font: 20rpx;
}
